{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../../",
  "testTimeout": 30000,
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1"
  },
  "globals": {
    "LOGGING_DISABLED": true
  }
}
