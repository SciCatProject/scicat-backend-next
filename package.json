{
  "name": "scicat-backend-next",
  "version": "4.0.0",
  "description": "scicat-backend-next",
  "author": "",
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "posttest": "npm run lint",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e:restart:db": "docker-compose rm -s -f -v test-db  && docker-compose up -d test-db && sleep 1",
    "test:e2e": "npm run pretest:e2e:restart:db && dotenv -e .env.test -- jest --watch=false --no-cache --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@user-office-software/duo-message-broker": "^1.3.0",
    "bcrypt": "^5.0.1",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "mathjs": "^9.5.2",
    "mongoose": "^6.3.3",
    "nodemailer": "^6.7.5",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-ldapauth": "^3.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.180",
    "@types/luxon": "^2.3.0",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "dotenv-cli": "^5.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "pactum": "^3.1.7",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
